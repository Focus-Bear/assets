    "shutOffTime": {
      "$id": "#/properties/shutOffTime",
      "type": "string",
      "title": "Shutoff time",
      "description": "When you will turn off your computer (24hr format).",
      "default": "20:00",
      "pattern": "^([01][0-9]|2[0-3]):([0-5][0-9])$",
      "examples": ["19:15"]
    },
