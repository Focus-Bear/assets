{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://earnyourscreentime.com/config/settings-schema.json",
  "type": "object",
  "title": "EYST settings schema",
  "description": "Schema for settings for Earn Your Screen Time",
  "default": {},
  "examples": [
    {
      "shutOffTime": "19:15",
      "morningHabits": [
        {
          "name": "Yoga",
          "durationSeconds": 360,
          "videoUrl": "https://www.youtube.com/watch?v=4C-gxOE0j7s"
        },
        {
          "name": "Brush teeth",
          "durationSeconds": 120
        }
      ],
      "breakAfterMinutes": 15,
      "startTime": "4:50",
      "breakActivities": [
        {
          "name": "Mindful breathing",
          "videoUrl": "https://www.youtube.com/watch?v=z-3n5iBi4u0",
          "durationSeconds": 30
        },
        {
          "name": "Calisthenics",
          "durationSeconds": 30,
          "includeInEveryBreak": true
        },
        {
          "name": "Chores",
          "durationSeconds": 30
        }
      ]
    }
  ],
  "required": [
    "shutOffTime",
    "morningHabits",
    "breakAfterMinutes",
    "startTime",
    "breakActivities"
  ],
